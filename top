<script>
// SLIDER
let cur=0;
const slides=document.querySelectorAll('.slide');
const dots=document.querySelectorAll('.hdot');
function goSlide(n){slides[cur].classList.remove('on');dots[cur].classList.remove('on');cur=n;slides[cur].classList.add('on');dots[cur].classList.add('on');}
setInterval(()=>goSlide((cur+1)%slides.length),4500);

// STICKY NAV â€” apare dupa ce treci de hero
window.addEventListener('scroll',()=>{
  document.getElementById('stickynav').classList.toggle('show', window.scrollY > window.innerHeight * 0.75);
});

// CART
let cart=[];
function addToCart(name,price){cart.push({name,price});updateCart();showToast('âœ“ '+name+' adÄƒugat!');if(!document.getElementById('cpanel').classList.contains('on'))toggleCart();}
function updateCart(){
  const n=cart.length,total=cart.reduce((s,i)=>s+i.price,0);
  const b=document.getElementById('cbadge');b.textContent=n;b.classList.toggle('on',n>0);
  const body=document.getElementById('cbody'),ftr=document.getElementById('cftr');
  if(!n){body.innerHTML='<div class="cempty"><span class="ico">ğŸ›’</span><p>CoÈ™ul este gol.</p></div>';ftr.style.display='none';return;}
  body.innerHTML=cart.map((item,i)=>`<div class="ci-item"><div class="ci-thumb">${getE(item.name)}</div><div class="ci-info"><div class="ci-name">${item.name}</div><div class="ci-price">${item.price} Lei</div></div><div class="ci-rm" onclick="rmItem(${i})">âœ•</div></div>`).join('');
  document.getElementById('ctotal').textContent=total+' Lei';ftr.style.display='block';
}
function getE(n){if(n.includes('Rose'))return'ğŸŒ¸';if(n.includes('Green'))return'ğŸŒ¿';if(n.includes('Citrus'))return'ğŸŠ';if(n.includes('Sensitiv'))return'ğŸ’œ';if(n.includes('Coconut'))return'ğŸ¥¥';if(n.includes('White'))return'ğŸ¤';if(n.includes('Black'))return'ğŸ–¤';return'ğŸ§´';}
function rmItem(i){cart.splice(i,1);updateCart();}
function toggleCart(){document.getElementById('cpanel').classList.toggle('on');document.getElementById('cov').classList.toggle('on');}

// NAV
function navTo(id){const el=document.getElementById(id);if(el)el.scrollIntoView({behavior:'smooth'});}

// FILTER
function filterP(cat,btn){document.querySelectorAll('.ftab').forEach(t=>t.classList.remove('on'));btn.classList.add('on');document.querySelectorAll('.pc').forEach(c=>{c.style.display=(cat==='toate'||c.dataset.cat===cat)?'block':'none';});}

// TOAST
function showToast(msg){const t=document.getElementById('toast');t.textContent=msg;t.classList.add('on');setTimeout(()=>t.classList.remove('on'),3000);}
</script>
